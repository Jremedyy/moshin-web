{
  "name": "moshin-media",
  "packageManager": "yarn@1.22.21",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-12-06 --compatibility-flag=nodejs_compat --port=3000"
  },
  "dependencies": {
    "next": "14.1.0",
    "prop-types": "^15.8.1",
    "react": "^18",
    "react-dom": "^18",
    "react-lottie": "^1.2.4",
    "react-select": "^5.8.0",
    "styled-components": "^6.1.8",
    "svg-url-loader": "^8.0.0",
    "swiper": "^11.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@cloudflare/next-on-pages": "^1.11.0",
    "@cloudflare/workers-types": "^4.20240405.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-lottie": "^1.2.10",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-next-on-pages": "^1.11.0",
    "svg-url-loader": "^8.0.0",
    "typescript": "^5",
    "vercel": "^33.5.5",
    "wrangler": "^3.33.0"
  },
  "resolutions": {
    "string-width": "4.2.3"
  }
}
